package Test4;
public class Ques1 {
	public static void main(String[] args) {
		int arr[]= {1,1,2,2,2,5};
		int x=0;
		for(int i=0;i<arr.length;i++)
		{
			if(x==arr[i])
			{
				continue;
			}
			else
			{
				x=arr[i];
				System.out.print(x+" ");
			}
		}
	}
}
package Test4;
public class Ques2 {
	public static void main(String[] args) {
		int arr[]= {100,200,300,400};
		int k=2;
		int n=arr.length;
		int currsum=0;
		int sum=0; //as it is positive only
		for(int i=0;i<n;i++)
		{
			int j=currsum;
			currsum=currsum+arr[i];
			if(currsum>sum)
			{
				sum=currsum;
				
			}
			currsum=currsum-j;
		}
		System.out.println(sum);
	}
}
